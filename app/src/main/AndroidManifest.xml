<manifest android:versionCode="1" android:versionName="1.0"
    package="com.example.apple.samplecode"
    xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <uses-sdk android:minSdkVersion="16" android:targetSdkVersion="19" tools:overrideLibrary="com.myappconverter.java"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application android:allowBackup="true"
        android:icon="@drawable/ic_launcher_default"
        android:label="@string/Touches" android:name="APLAppDelegate"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" tools:replace="android:icon,android:theme,android:label,android:name">
        <activity android:name="LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>
</manifest>
